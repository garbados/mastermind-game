{
  "name": "mastermind-game",
  "version": "0.1.1",
  "description": "Play Mastermind in your terminal",
  "main": "index.js",
  "scripts": {
    "jshint": "./node_modules/.bin/jshint bin lib test",
    "mocha": "./node_modules/.bin/mocha -R spec --recursive",
    "cov": "./node_modules/.bin/jscoverage lib cov",
    "report": "./node_modules/.bin/mocha -R html-cov > index.html",
    "test": "npm run jshint && npm run mocha && npm run cov && npm run report"
  },
  "bin": {
    "mastermind": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garbados/mastermind-game.git"
  },
  "author": "Diana Thayer <me@dianathayer.org>",
  "license": "ISC",
  "devDependencies": {
    "chai": "^1.10.0",
    "jscoverage": "^0.5.9",
    "jshint": "^2.5.11",
    "mocha": "^2.1.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "prompt": "^0.2.14",
    "underscore": "^1.7.0",
    "yargs": "^1.3.3"
  }
}
